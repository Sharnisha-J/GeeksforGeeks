class Solution {
    // Function to find maximum product subarray
    int maxProduct(int[] arr) {
        // code here
        int n=arr.length;
        int max=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            int ans=1;
            for(int j=i;j<n;j++){
                ans*=arr[j];
                max=Math.max(max,ans);
            }
        }
        return max;
    }
}
