/*
class Node{
    int data;
    Node next;

    Node(int x){
        data = x;
        next = null;
    }
}
*/
class Solution {
    public Node insertAtEnd(Node head, int x) {
        // code here
        if(head==null){
            Node t=new Node(x);
            head=t;
            return head;
        }
        Node p=head;
        while(p.next!=null){
            p=p.next;
        }
        Node t=new Node(x);
        p.next=t;
        return head;
    }
}
